@{
    ViewData["Title"] = "Welcome to our sports data";
}


<div class="text-center">
    Welcome to our sports data
</div>
<div class="text-center">
    <img src=@Url.Content("data:image/png;base64,") />
</div>

<div class="container">
    <div class="row">
        <div class="col-sm">
            <a class="nav-link text-dark" asp-area="" asp-controller="Team" asp-action="AllTeams">All Teams Listed</a>
        </div>
        <div class="col-sm">
            <a class="nav-link text-dark" asp-area="" asp-controller="Coach" asp-action="AllCoaches">All Coaches Listed</a>
        </div>
        <div class="col-sm">
            <a class="nav-link text-dark" asp-area="" asp-controller="Stadium" asp-action="AllStadiums">All Stadiums Listed</a>
        </div>
    </div>
</div>
